<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CollapsingTitleLayout">
        <attr name="ctl_textTitle" format="string|reference" />
        <attr name="ctl_textSubtitle" format="string|reference" />
        <attr name="ctl_titleTextSize" format="dimension" />
        <attr name="ctl_subtitleTextSize" format="dimension" />
        <attr name="ctl_fontFamily" format="string|reference" />
        <attr name="ctl_titleColor" format="color" />
        <attr name="ctl_subtitleColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CollapsingViewBehavior">
        <attr name="target_place" format="reference" />
    </declare-styleable>

</resources>